CC = gcc
LD = gcc
CFLAGS = -c -O3
LDFLAGS = -o -O3

OBJS =  modelagem2D.o parametros.o modeling_data.o  readFile.o update_wave_field.o wave_equation.o writeToFile.o apply_Reynolds_Boundary.o vel_model.o apply_Cerjan_Boundary.o calc_coef_damping_zones.o geometria.o

MODELAGEM2D = modelagem2D

EXECS=$(MODELAGEM2D)

.c.o:
	$(CC) $(CFLAGS) $< 

all:$(EXECS)

modelagem2D:$(OBJS)

clean:
	rm -f *.o $(EXECS)
